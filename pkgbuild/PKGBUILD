pkgname='Run-wrapper'
pkgver='0.0.5'
pkgrel='1'
pkgbase="$pkgname"
pkgdesc='ELF wrapper for RunImage Run.sh script in the extracted form'
url="https://github.com/VHSgunzo/Run-wrapper"
arch=('x86_64')
license=('MIT')
source=('Run')
sha256sums=('SKIP')

package() {
  install -Dm755 Run "${pkgdir}/var/RunDir/Run"
}